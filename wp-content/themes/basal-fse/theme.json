{
	"$schema": "https://schemas.wp.org/trunk/theme.json",
	"version": 1,
	"customTemplates": [
		{
			"name": "blank",
			"title": "Blank",
			"postTypes": [
				"page",
				"post"
			]
		},
		{
			"name": "fullwidth",
			"title": "Full Width",
